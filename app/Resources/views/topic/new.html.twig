{% extends 'base.html.twig' %}

{% block body %}

    <h2>Create a new topic</h2>

    {{ form_start(newTopicForm) }}

        {{ form_row(newTopicForm.title) }}
        {{ form_row(newTopicForm.message) }}

        <button type="submit" class="btn btn-success">Create <span class="fa fa-lock"></span></button>

    {{ form_end(newTopicForm) }}

{% endblock %}